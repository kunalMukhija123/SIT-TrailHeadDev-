<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Room_Price_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>!AND( Price__c &gt;= 1000 ,  Price__c &lt;= 10000)</errorConditionFormula>
    <errorMessage>Price Should between 1000 to 10000</errorMessage>
</ValidationRule>
